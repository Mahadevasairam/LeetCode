class Solution {
    public int diagonalSum(int[][] mat) {
        int n = mat.length;int sum =0;
            for(int i = 0;i<n;i++){
                for (int j =0;j<n;j++){
                    if(i == j) {sum = sum + mat[i][j];
                                System.out.println(sum);}
                    else if(n-(i+1)==j){sum = sum + mat[i][j];
                                   System.out.println(sum);}
                }
            }
      
     return sum;   
    }
}
