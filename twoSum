class Solution {
    public int[] twoSum(int[] nums, int target) {
        int sum = nums[1];
        int[] array = new int[2];
        int count = 0;
        for(int i =0;i <nums.length;i++){
            sum = nums[i];
                for(int j =i+1;j<nums.length;j++){
                    if(target == nums[j]+nums[i]){
                        array[0] = i;
                        array[1] = j;
                    }
              }
         }
    return array;
    }
}
